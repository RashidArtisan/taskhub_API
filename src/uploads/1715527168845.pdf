// Using a for loop
console.log("Using for loop:");
for (let i = 1; i <= 100; i++) {
    console.log(i);
}

// Using a do-while loop
console.log("Using do-while loop:");
let j = 1;
do {
    console.log(j);
    j++;
} while (j <= 100);

// Using a while loop
console.log("Using while loop:");
let k = 1;
while (k <= 100) {
    console.log(k);
    k++;
}
